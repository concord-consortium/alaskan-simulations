{
  "name": "common",
  "version": "0.0.1",
  "description": "FOSS Simulation common components and helpers",
  "main": "src/index.ts",
  "jest": {
    "testURL": "https://foss-simulations.unexisting.url.com",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "config": {
    "title": "common"
  },
  "scripts": {
    "build": "yarn run lint:build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \"../.eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/foss-simulations.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/foss-simulations/issues"
  },
  "homepage": "https://github.com/concord-consortium/foss-simulations#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@types/react-table": "^7.7.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "filemanager-webpack-plugin": "^7.0.0",
    "hook-shell-script-webpack-plugin": "^0.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@dnd-kit/core": "^5.0.3",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.2",
    "@types/deep-equal": "^1.0.1",
    "clsx": "^1.1.1",
    "deep-equal": "^2.0.5",
    "eslint-plugin-jest": "^26.1.2",
    "focus-trap": "^6.9.4",
    "query-string": "^7.1.1",
    "react-markdown": "^8.0.2",
    "react-table": "^7.8.0",
    "rehype-raw": "^6.1.1"
  }
}
