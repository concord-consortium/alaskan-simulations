    "@react-three/fiber": "^8.13.4",
    "camera-controls": "^1.36.2",
    "clsx": "^1.2.1",
    "common": "*",
    "eslint-plugin-jest": "^26.1.2",
    "jquery": "^3.6.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shutterbug": "^1.5.0",
    "suncalc": "^1.9.0",
    "three": "^0.154.0"
  }
}
